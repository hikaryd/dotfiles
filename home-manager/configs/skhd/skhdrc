alt - 1 : komorebic focus-named-workspace 1
alt - 2 : komorebic focus-named-workspace 2
alt - 3 : komorebic focus-named-workspace 3
alt - 4 : komorebic focus-named-workspace 4
alt - d : komorebic focus-named-workspace d
alt - e : komorebic focus-named-workspace e
alt - q : komorebic focus-named-workspace q
alt - t : komorebic focus-named-workspace t
alt - w : komorebic focus-named-workspace w
alt - tab : komorebic focus-last-workspace

alt + shift - 1 : komorebic send-to-named-workspace 1
alt + shift - 2 : komorebic send-to-named-workspace 2
alt + shift - 3 : komorebic send-to-named-workspace 3
alt + shift - 4 : komorebic send-to-named-workspace 4
alt + shift - d : komorebic send-to-named-workspace d
alt + shift - e : komorebic send-to-named-workspace e
alt + shift - q : komorebic send-to-named-workspace q
alt + shift - t : komorebic send-to-named-workspace t
alt + shift - w : komorebic send-to-named-workspace w
alt + shift - tab : komorebic cycle-move-workspace-to-monitor forward

# Фокус и перемещение по сетке
alt - h : komorebic focus left
alt - j : komorebic focus down
alt - k : komorebic focus up
alt - l : komorebic focus right
alt + shift - h : komorebic move left
alt + shift - j : komorebic move down
alt + shift - k : komorebic move up
alt + shift - l : komorebic move right

alt + shift - 0x18 : komorebic resize-axis horizontal increase
alt + shift - 0x1B : komorebic resize-axis horizontal decrease

# Балансировка и режимы
alt + shift - 0 : komorebic retile
alt + shift - f : komorebic toggle-monocle
cmd + shift - f : komorebic toggle-float
cmd + shift - e : komorebic change-layout ultrawide-vertical-stack
cmd + shift - h : komorebic stack left
cmd + shift - l : komorebic stack right
cmd + shift - r : komorebic unstack-all && komorebic retile

# Stack
alt - left        : komorebic stack left
alt - down        : komorebic stack down
alt - up          : komorebic stack up
alt - right       : komorebic stack right

# пунктуация — только hex keycodes
alt - 0x29        : komorebic unstack              # ; 
alt - 0x21        : komorebic cycle-stack previous # [
alt - 0x1E        : komorebic cycle-stack next     # ]

alt + shift - 0x21 : komorebic cycle-stack-index previous # [
alt + shift - 0x1E : komorebic cycle-stack-index next     # ]
